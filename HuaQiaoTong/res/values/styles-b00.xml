<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="MainNavBg">
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

        <style name="ListBackContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/com_transparent</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ListFrontContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/com_transparent</item>
    </style>

    <style name="ListButtonAction">
        <item name="android:layout_margin">10dp</item>
    </style>
    
    
    <style name="MainNavBgSub">
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@color/main_nav_bg_m</item>
    </style>

    <style name="MainItemTextStyle" parent="@style/B01v03ImpTextTheme">
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="B00v00ItemInfoStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="B00v00ButtonPanalStyle">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@color/b00_v00_panal_bg</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:padding">5dp</item>
    </style>
    
    <style name="B00v00ButtonPanalStyle2">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="B00v00ButtonTextStyle2">
        <item name="android:textColor">@color/b00_v00_b_text_color_0</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="b00v00HistoryDialogStyle" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="b00RightBtnStyle">
        <item name="android:layout_weight">1</item>
        <item name="android:padding">2dp</item>
        <item name="android:gravity">bottom|center_horizontal</item>
        <item name="android:textColor">@color/main_right_text_color</item>
    </style>
    
    <style name="b00RightBtnStyle2">
        <item name="android:layout_weight">1</item>
        <item name="android:padding">13dp</item>
       <item name="android:textSize">22sp</item>
        <item name="android:gravity">bottom|center_horizontal</item>
        <item name="android:textColor">@color/main_right_text_color</item>
    </style>

</resources>